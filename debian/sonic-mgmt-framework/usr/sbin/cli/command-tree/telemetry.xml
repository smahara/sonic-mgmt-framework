<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE CLISH_MODULE [
<!ENTITY START_PORT_ID "1">
<!ENTITY MAX_PORT_ID "32">
<!ENTITY START_SUB_PORT_ID "1">
<!ENTITY MAX_SUB_PORT_ID "4">
<!ENTITY MAX_MTU "9276">
<!ENTITY VLAN_NUM "(409[0-3]|40[0-8][0-9]|[1-3][0-9]{3}|[1-9][0-9]{2}|[1-9][0-9]|[1-9])">
]>
<CLISH_MODULE xmlns="http://www.dellemc.com/sonic/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xi="http://www.w3.org/2001/XInclude" xsi:schemaLocation="http://www.dellemc.com/sonic/XMLSchema http://www.dellemc.com/sonic/XMLSchema/clish.xsd">
  <!--=========================================================================-->
  <!-- Show watermark telemetry interval -->
  <VIEW name="enable-view">
    <COMMAND name="show watermark" help="show watermark telemetry"/>
    <COMMAND name="show watermark telemetry" help="Show watermark telemetry information">
      <PARAM name="interval" help="Displays the configured interval for the telemetry" ptype="SUBCOMMAND" mode="subcommand" optional="true"> </PARAM>
      <ACTION> $SONIC_CLI_ROOT/sonic-cli-telemetry.py get_sonic_watermark_telemetry_watermark_table_interval show_telemetry.j2</ACTION>
      <xi:include href="include/pipe.xml"/>
      <xi:include href="include/pipe.xml"/>
    </COMMAND>
  </VIEW>
  <!--Configure watermark telemetry interval <seconds> -->
  <VIEW name="configure-view">
    <COMMAND name="watermark" help="Configure watermark telemetry"/>
    <COMMAND name="watermark telemetry" help="Configure watermark telemetry interval">
      <PARAM name="interval" help="configure interval for telemetry" ptype="SUBCOMMAND" mode="subcommand">
        <PARAM name="seconds" help="number of seconds. Default interval is 120 seconds" ptype="UINT16"/>
      </PARAM>
      <ACTION>$SONIC_CLI_ROOT/sonic-cli-telemetry.py patch_sonic_watermark_telemetry_watermark_table_interval ${seconds} </ACTION>
    </COMMAND>
  </VIEW>
</CLISH_MODULE>
