            <PARAM
                name="drop-probability"
                help="Maximum drop rate keyword"
                mode="subcommand"
                ptype="SUBCOMMAND">
                <PARAM
                    name="max-drop-rate"
                    help="Specify maximum in drop rate in percent"
                    ptype="PERCENT"
                    />
            </PARAM>
        </PARAM>
    </PARAM>
</MACRODEF>

</ROOT>
