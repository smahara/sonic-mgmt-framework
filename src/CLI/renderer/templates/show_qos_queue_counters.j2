{{'-------------------------------------------------------------------'}}
{{'%-12s'|format("Port")}}{{'%-5s'|format("TxQ")}}{{'%-15s'|format("Counter/pkts")}}{{'%-15s'|format("Counter/bytes")}}{{'%-12s'|format("Drop/pkts")}}{{'%-12s'|format("Drop/bytes")}}
{{'-------------------------------------------------------------------'}}
{% if json_output %}
{% set intf_info_list = json_output['interface'] %}
{% for intf_info in intf_info_list %}
  {% set intfName = intf_info['interface-id'] %}
  {% set output = intf_info['output'] %}
  {% set queues = output['queues'] %}
  {% set queue_info_list = queues['queue'] %}
  {% for queue_info in queue_info_list %}
    {% set q_stats_info = queue_info['state'] %}
    {% set queue = q_stats_info['name'] %}
    {% set traffic_type = q_stats_info['openconfig-qos-ext:traffic-type'] %}
    {% set queue_name = traffic_type + queue %}
    {% set packets = q_stats_info['transmit-pkts'] %}
    {% set bytes = q_stats_info['transmit-octets'] %}
    {% set droppedPackets = q_stats_info['dropped-pkts'] %}
    {% set droppedBytes = q_stats_info['openconfig-qos-ext:dropped-octets'] %}
{{'%-12s'|format(intfName)}}{{'%-5s'|format(queue_name)}}{{'%-15s'|format(packets)}}{{'%-15s'|format(bytes)}}{{'%-12s'|format(droppedPackets)}}{{'%-12s'|format(droppedBytes)}}
  {% endfor %}
{% endfor %}
{% endif %}
